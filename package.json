{
  "name": "@ustaxcourt/ustc-pay-gov-test-server",
  "version": "0.0.0",
  "description": "USTC Pay.gov test server",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ustaxcourt/ustc-pay-gov-test-server"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npx tsc",
    "dev": "NODE_ENV='local' concurrently \"npx tsc --watch\" \"nodemon -q ./dist/index.js\"",
    "copy-files": "aws s3 cp src/static s3://dev-ustc-pay-gov-test-server --recursive",
    "test": "jest ./src",
    "start": "node .",
    "test:integration": "NODE_ENV='local' jest --verbose --bail ./test/integration",
    "test:integration:prod": "DOTENV_CONFIG_PATH=.env.prod NODE_ENV='prod' jest --bail ./test/integration",
    "ci:publish": "changeset publish --provenance"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bin": {
    "start-pay-gov-test-server": "bin/start-pay-gov-test-server.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "express": "^4.18.2",
    "fast-xml-parser": "^5.2.5",
    "lodash": "^4.17.21",
    "luxon": "^3.7.2",
    "soap": "^1.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/aws-lambda": "^8.10.152",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/luxon": "^3.7.1",
    "@types/uuid": "^9.0.8",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.2",
    "esbuild": "^0.25.9",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
